{
  "name": "@oneclickdapp/ethereum-auth",
  "version": "0.2.1",
  "description": "Ethereum wallet login provider for RedwoodJS projects",
  "author": "Patrick Gallagher <blockchainbuddha@gmail.com>",
  "module": "src/index.js",
  "lint-staged": {
    "*.js": []
  },
  "prepublish": "yarn build",
  "scripts": {
    "precommit": "lint-staged",
    "watch": "webpack --watch --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --progress",
    "test": "jest",
    "lint": "eslint ./src --fix",
    "stats-bundle": "webpack --config webpack.prod.js --profile --json > stats.json && bundle-stats ./stats.json",
    "stats-baseline": "bundle-stats ./stats.json --baseline --json",
    "stats": "yarn stats-bundle && xdg-open ./dist/bundle-stats.html && webpack-bundle-analyzer ./stats.json ./dist"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "@ethersproject/bytes": "^5.0.10",
    "@ethersproject/keccak256": "^5.0.8",
    "@ethersproject/providers": "^5.0.4",
    "@ethersproject/strings": "^5.0.9",
    "@walletconnect/web3-provider": "^1.5.1",
    "assert": "^2.0.0",
    "crypto-browserify": "^3.12.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime-corejs3": "^7.15.3",
    "babel-loader": "^8.0.6",
    "bundle-stats": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "lint-staged": "^10.2.2",
    "prettier": "^1.19.1",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oneclickdapp/ethereum-auth"
  },
  "homepage": "https://github.com/oneclickdapp/ethereum-auth"
}
